{{#each channel}}
{{#gte @index compare=../index_start}}
{{#gte @index compare=../index_end}}
{{else}}
<div class="editors__item">
	<div class="editors__img">
		<a href="artikel.html">
			<img src="https://asset.kompas.com/crop/0x0:0x0/195x117/data/photo/{{img}}" alt="{{title}}">
		</a>
	</div>
	<div class="editors__box">
		<h4 class="editors__subtitle"><a href="kanal.html">{{subtitle}}</a></h4>
		<h2 class="editors__title">
			<a href="artikel.html" class="editors__link">{{title}}</a>
		</h2>
		<date class="editors__date">25 September 2019, 23:12 WIB</date>
	</div>
</div>
{{/gte}}
{{else}}
{{/gte}}
{{/each}}
