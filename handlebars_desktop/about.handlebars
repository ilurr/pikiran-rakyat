<!DOCTYPE html>
<html lang="en">
	{{> head}}

<body>
	<!-- header -->
	{{> header}}
	
	<!-- ads skin -->
	{{> ads_skin}}
	
	<!-- content -->
	<div class="row clearfix">
		<div class="container clearfix">
			<div class="col-offset-fluid clearfix">
				<div class="col-bs12-12">
					
					<!-- ads top -->
					{{> ads_top}}
					
					{{> read_breadcrumb channel=read.static.about}}

				</div>
				
				<div class="col-bs12-8">
				
					<!-- header -->
					<section class="read__header clearfix">
						{{> read_header channel=read.static.about}}
					</section>
					
					<div class="read__article clearfix">
						<div class="col-bs12-12">
						
							<!-- content -->
							<article class="read__content clearfix">
								{{{read.static.about.content}}}
							</article>
							
						</div>
					</div>
					
					
				</div>
				<div class="col-bs12-4">
				
					<!-- ads square -->
					<div class="ads mt3 clearfix">
						{{> ads_square}}
					</div>
					
					<!-- ads square -->
					<div class="ads mt3 clearfix">
						{{> ads_square}}
					</div>
					
				</div>
			</div>
		</div>
	</div>
	
	<!-- footer -->
	{{> footer}}

	<!-- ads horizontal -->
	{{> ads_horizontal}}

	{{> js}}
	
</body>
</html>
