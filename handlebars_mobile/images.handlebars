<!DOCTYPE html>
<html lang="en">
	{{> head}}

<body>
	<!-- header -->
	{{> header}}
	
	<!-- nav -->
	{{> nav}}
	
	<!-- content -->
	<div class="row clearfix">
		<div class="container clearfix">
					
			<!-- ads top -->
			{{> ads_top}}

			<h3 class="title4"><span>PR Images</span></h3>

			<!-- headline -->
			<section class="photos__hl clearfix">
				{{> headline_images channel=channel.images.features total=4 index_start=0 index_end=1}}
			</section>

			<!-- latest -->
			<section class="latest latest--photos clearfix">
				<div class="latest__wrap">
					{{> latest_images channel=channel.images.photo total=8 index_start=0 index_end=8}}
				</div>
			</section>

			<!-- ads square -->
			<div class="ads mt2 clearfix">
				{{> ads_square}}
			</div>

			<!-- latest -->
			<section class="latest latest--photos mt2 clearfix">
				<div class="latest__wrap">
					{{> latest_images channel=channel.images.photo total=8 index_start=8 index_end=16}}
				</div>
			</section>

			<!-- ads square -->
			<div class="ads mt2 clearfix">
				{{> ads_square}}
			</div>

			<!-- latest -->
			<section class="latest latest--photos mt2 clearfix">
				<div class="latest__wrap">
					{{> latest_images channel=channel.images.photo total=8 index_start=16 index_end=32}}
				</div>

				<div class="latest__more clearfix">
					<a href="#" class="latest__button">Lihat Semua</a>
				</div>
			</section>

			<!-- ads square -->
			<div class="ads mt2 clearfix">
				{{> ads_square}}
			</div>
					
		</div>
	</div>
	
	<!-- footer -->
	{{> footer}}

	{{> js}}
	
</body>
</html>
